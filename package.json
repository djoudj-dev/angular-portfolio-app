{
  "name": "angular-portfolio-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "serve:ssr:angular-portfolio-app": "node dist/angular-portfolio-app/server/server.mjs",
    "lint": "eslint . --ext .ts,.js,.html",
    "lint:fix": "eslint . --ext .ts,.js,.html --fix",
    "prettier": "prettier --write \"src/**/*.{ts,js,html,css,scss}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,js,html,css,scss}\"",
    "format": "npm run prettier && npm run lint:fix",
    "pre-commit": "npm run prettier:check && npm run lint && npm run test:ci",
    "test:ci": "vitest run",
    "commit": "git-cz",
    "commit:fr": "git-cz --config .czrc",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.5",
    "@supabase/supabase-js": "^2.50.3",
    "@tailwindcss/postcss": "^4.1.11",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.19.1",
    "@analogjs/vite-plugin-angular": "^1.19.1",
    "@analogjs/vitest-angular": "^1.19.1",
    "@angular-eslint/builder": "^20.1.1",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/build": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.1",
    "@nx/vite": "^21.0.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.7.0",
    "jsdom": "^22.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.2",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^3.0.0"
  }
}
